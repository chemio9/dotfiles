# vim:ft=bash

zmodule environment
zmodule input
zmodule termtitle
zmodule utility
zmodule archive
# zmodule le0me55i/zsh-extract -s 'extract.plugin.zsh'

zmodule romkatv/powerlevel10k --use degit

zmodule zsh-users/zsh-completions --fpath src
zmodule completion

zmodule zsh-users/zsh-autosuggestions
zmodule zdharma/fast-syntax-highlighting -s 'F-Sy-H.plugin.zsh'
zmodule zsh-users/zsh-history-substring-search -s 'zsh-history-substring-search.zsh' --cmd " \
  bindkey \"^[OA\" history-substring-search-up ;\
  bindkey \"^[OB\" history-substring-search-down "

zmodule MichaelAquilina/zsh-you-should-use
zmodule skywind3000/z.lua --cmd 'eval "$(command -v lua > /dev/null && lua {}/z.lua --init zsh enhanced once)"'

# zmodule Aloxaf/fzf-tab --if 'where fzf >>/dev/null'
zmodule hlissner/zsh-autopair
